{
   "background": {
      "scripts": [ "background.js" ]
   },
   "description": "Copy text in web page as markdown quotation",
   "manifest_version": 2,
   "minimum_chrome_version": "24",
   "name": "Copy as Markdown Quotation",
    "icons": { "16": "icon16.png",
               "48": "icon48.png",
              "128": "icon128.png" },
   "permissions": [ "tabs", "storage", "contextMenus", "notifications", "http://*/", "https://*/" ],
   "version": "0.4.0"
}
